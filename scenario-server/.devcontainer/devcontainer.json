{
  "name": "AIsenpai-Scenario-Server",
  "dockerComposeFile": [
    "../../docker-compose.yml"
  ],
  "service": "scenario-server",
  "shutdownAction": "none",
  "workspaceFolder": "/workspace/scenario-server",
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "eamodio.gitlens",
    "mhutchie.git-graph"
  ],
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true
  }
}